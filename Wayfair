function cmp(a, b) { return a - b; }

function solution(A, B) {
    var n = A.length;
    var m = B.length;
    A.sort(cmp);
    B.sort(cmp);
    console.log(A, B)
    var i = 0;
    for (var k = 0; k < n; k++) {
        if (i < m - 1 && B[i] < A[k])
            i += 1, k-=1;
        if (A[k] == B[i])
            return A[k];
    }
    return -1;
}

solution([1,2,3, 4, 8, 10],[7,8,12])